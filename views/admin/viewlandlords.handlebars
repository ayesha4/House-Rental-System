<div>
<p>Registered Landlords</p>
<table class="atable table">
  <thead>
    <tr>
      <th scope="col">Name</th>
      <th scope="col">Email</th>
      <th scope="col">Phone</th>
      <th scope="col">Action</th>
    </tr>
  </thead>
  <tbody>
    {{#each data}} 
    <tr>
      
      <td >{{Name}}</td>
      <td>{{email}}</td>
      <td>{{phone}}</td>
       <td>
                        <form action="/admin/viewlandlords/{{uid}}" method="POST">
                        <button type="submit" class="view">DELETE</button>
                    </form> 
                    </td>
    </tr>
    
     {{/each}}
  </tbody>
</table>


</div></div>
</body>
</html>